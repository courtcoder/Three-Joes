<div class="container xcolumns mx-auto">

  <div class="signup-panel">
    <form class="signup-form p-2">
      <h1>Signup</h1>
      <div>
        {{!-- <label for="username-signup">Username:</label> --}}
        <input class="p-3" type="text" id="username-signup" placeholder="Username"/>
      </div>
      <div>
        {{!-- <label for="first-name-signup">First Name:</label> --}}
        <input class="p-3" type="text" id="first-name-signup" placeholder="First name" />
      </div>
      <div>
        {{!-- <label for="last-name-signup">Last Name:</label> --}}
        <input class="p-3" type="text" id="last-name-signup" placeholder="Last name" />
      </div>
      <div>
        {{!-- <label for="email-signup">E-mail:</label> --}}
        <input class="p-3" type="text" id="email-signup" placeholder="Email" />
      </div>
      <div>
        {{!-- <label for="password-signup">Password:</label> --}}
        <input class="p-3" type="password" id="password-signup" placeholder="Password"/>
      </div>
      <div>
        <button class="button is-info" type="submit">Sign Up</button>
      </div>
    </form>
  </div>

</div>





<script src="/javascript/signup.js"></script>
